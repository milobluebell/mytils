{
  "name": "mytils",
  "version": "2.0.0",
  "description": "one useful utilities",
  "scripts": {
    "start": "per-env",
    "start:doc": "npx docsify serve docs",
    "start:development": "npm run -s dev",
    "dist": "rm -rf es dist && npx rollup -c",
    "dev": "preact watch",
    "lint": "./scripts/lint.sh",
    "test": "jest ./src -c --coverage",
    "pretty-quick": "pretty-quick",
    "pre-commit": "npm run pretty-quick && npm run lint",
    "clean": "rm -rf es lib dist .tsc-check coverage",
    "check-coverage": "npx istanbul check-coverage --statement 90"
  },
  "dependencies": {
    "dayjs": "^1.8.28",
    "lodash-es": "^4.17.15"
  },
  "peerDependencies": {
    "dayjs": "^1.8.25",
    "lodash-es": "^4.17.15"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^8.0.1",
    "@rollup/plugin-replace": "^2.3.3",
    "@types/jest": "^25.2.3",
    "@types/lodash-es": "^4.17.3",
    "@types/webpack-env": "^1.15.2",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "axios": "^0.20.0",
    "babel-preset-es2015": "^6.24.1",
    "docsify": "^4.11.3",
    "docsify-cli": "^4.4.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-markdown": "^1.0.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "husky": "^4.2.5",
    "identity-obj-proxy": "^3.0.0",
    "istanbul": "^0.4.5",
    "jest": "^25.5.4",
    "per-env": "^1.0.2",
    "preact": "^10.4.4",
    "preact-cli": "^3.0.0-rc.14",
    "preact-jsx-chai": "^3.0.0",
    "preact-markup": "^2.0.0",
    "preact-render-spy": "^1.3.0",
    "preact-render-to-string": "^5.1.9",
    "preact-router": "^3.2.1",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "rollup": "^2.13.1",
    "rollup-plugin-analyzer": "^3.2.3",
    "rollup-plugin-terser": "^5.2.0",
    "rollup-plugin-typescript2": "^0.26.0",
    "sirv-cli": "^1.0.0-next.5",
    "ts-jest": "^25.5.1",
    "ts-loader": "^6.2.2",
    "typescript": "~3.9.5",
    "typescript-transform-paths": "^1.1.14"
  },
  "keywords": [
    "javascript",
    "utils",
    "nodeJs",
    "functions"
  ],
  "main": "dist/lib/index.js",
  "module": "es/index.js",
  "unpkg": "dist/umd/mytils.min.js",
  "typings": "dist/types/index.d.ts",
  "files": [
    "dist",
    "es",
    "typings",
    "LICENSE",
    "README.md"
  ],
  "sideEffects": false,
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS",
      "pre-commit": "npm run pre-commit"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "type-enum": [
        2,
        "always",
        [
          "feat",
          "fix",
          "docs",
          "style",
          "test",
          "revert",
          "perf",
          "refactor",
          "chore"
        ]
      ],
      "type-empty": [
        2,
        "never"
      ],
      "subject-empty": [
        2,
        "never"
      ],
      "subject-full-stop": [
        0,
        "never"
      ],
      "subject-case": [
        0,
        "never"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/milobluebell/mytils.git"
  },
  "contributors": [
    "Milo Ma <milobluebell@gmail.com> (https://github.com/milobluebell)"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/milobluebell/mytils/issues"
  },
  "homepage": "https://milobluebell.github.io/mytils"
}